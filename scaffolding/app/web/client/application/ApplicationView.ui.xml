<!-- AppImpl.ui.xml -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
	xmlns:my="urn:import:@base@.client.application">

	<g:FlowPanel>

		<my:Header />

		<b:Container>

			<b:Breadcrumbs>
				<b:ListItem text="one" />
				<b:ListItem text="two" />
				<b:ListItem text="three" />
			</b:Breadcrumbs>

			<b:Alert ui:field="alert" visible="false"/>

			<g:SimplePanel ui:field="main" />
		</b:Container>

		<my:Footer />

	</g:FlowPanel>

</ui:UiBinder>