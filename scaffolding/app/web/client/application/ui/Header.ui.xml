<!-- Navigation.ui.xml -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b='urn:import:org.gwtbootstrap3.client.ui'>

	<ui:with type="@base@.client.place.NameTokens" field="nameTokens"></ui:with>

	<b:Navbar>

		<b:NavbarHeader>
			<b:NavbarBrand>@module@</b:NavbarBrand>
			<b:NavbarCollapseButton dataTarget="#navbar-collapse"/>
		</b:NavbarHeader>
		
		<b:NavbarCollapse b:id="navbar-collapse">
		   <b:NavbarNav>
		       <b:ListItem active="true">Item 1</b:ListItem>
		       <b:ListItem>Item 2</b:ListItem>
		       <b:ListItem>Item 3</b:ListItem>
		       
		       <b:ListDropDown>
		           <b:AnchorButton dataToggle="DROPDOWN">DropDown</b:AnchorButton>
		           <b:DropDownMenu>
		               <b:ListItem>Dropdown 1</b:ListItem>
		               <b:ListItem>Dropdown 2</b:ListItem>
		               <b:ListItem>Dropdown 3</b:ListItem>
		           </b:DropDownMenu>
		       </b:ListDropDown>
		   </b:NavbarNav>
		   
		   <b:NavbarForm pull="LEFT">
		       <b:TextBox placeholder="Search" addStyleNames="col-lg-8" />
		   </b:NavbarForm>
		   
		   <b:NavbarText pull="RIGHT">
		       Signed in as <b:NavbarLink>John Snow</b:NavbarLink>
		   </b:NavbarText>
		</b:NavbarCollapse>
	</b:Navbar>

</ui:UiBinder>